<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="300.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.FirstWindowController">
   <children>
      <Label layoutX="24.0" layoutY="14.0" text="start window">
         <font>
            <Font name="Helvetica Neue" size="20.0" />
         </font>
      </Label>
      <GridPane layoutX="150.0" layoutY="105.0" AnchorPane.leftAnchor="100.0" AnchorPane.rightAnchor="50.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Button fx:id="changeWindow" mnemonicParsing="false" onAction="#changeWindow" text="Ok" GridPane.columnIndex="1" GridPane.rowIndex="2" />
            <Button fx:id="openNewWindow" mnemonicParsing="false" onAction="#openNewWindow" text="Ok" GridPane.columnIndex="1" />
            <Label text="new window" />
            <Label text="change window" GridPane.rowIndex="2" />
         </children>
      </GridPane>
   </children>
</AnchorPane>
